{
  "version": 2,
  "builds": [
      {
          "src": "**/*",
          "use": "@vercel/static"
      }
  ],
  "routes": [
      {
          "src": "/api/(.*)",
          "dest": "https://be-slacker.vercel.app/api/$1"
      },
      {
          "handle": "filesystem"
      },
      {
          "src": "/(.*)\\.(.+)",
          "dest": "/$1.$2"
      },
      {
          "src": "/(.*)",
          "dest": "/index.html"
      }
  ]
}